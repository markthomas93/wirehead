{
    "manifest_version": 2,
   
    "name": "Wirehead",
    "description": "This app helps you maintain your focus on the internet",
    "version": "1.0",
   
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "history",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["bundle.js"],
        "run_at": "document_start"
      }
    ]
  }