{
  "name": "wirehead",
  "version": "1.0.1",
  "description": "An intelligent productivity extension for Chrome",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "analyze-bundle": "npx webpack-bundle-analyzer compilation-stats.json public",
    "build-client": "webpack -p",
    "build-client-show-bundle-analysis": "webpack -p --profile --json > compilation-stats.json",
    "build-client-watch": "webpack -w",
    "build-theme": "cd client/semantic && gulp build && cd ../..",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "# lint-staged # un-comment to enable",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bayes-classifier": "0.0.5",
    "britecharts": "^2.9.8",
    "css-loader": "^1.0.1",
    "d3": "^5.8.2",
    "d3-format": "^1.3.2",
    "d3-selection": "^1.4.0",
    "dexie": "^2.0.4",
    "file-loader": "^2.0.0",
    "history": "^4.6.3",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-chrome-redux": "^2.0.0-alpha.4",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "semantic-ui-calendar-react": "^0.13.0",
    "semantic-ui-react": "^0.85.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "dsv-loader": "^2.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.0",
    "eslint": "^5.13.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "npm-merge-driver": "^2.3.5",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "semantic-ui": "^2.4.2",
    "supertest": "^3.4.2",
    "webpack": "^4.29.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirehead-extension/wirehead.git"
  },
  "bugs": {
    "url": "https://github.com/wirehead-extension/wirehead/issues"
  },
  "homepage": "https://github.com/wirehead-extension/wirehead#readme"
}
